.cu-card.article>.cu-item {
  padding-bottom: 10rpx;
  border-bottom: 1rpx solid #eeeeee;
  padding-bottom: 20rpx;
  box-shadow: 0 2px 4px rgba(0, 0, 0, .12), 0 0 6px rgba(0, 0, 0, .04);
}



.image.list{
  display: flex;
  /* box-shadow: 0 2px 4px rgba(0, 0, 0, .12), 0 0 6px rgba(0, 0, 0, .04) */
}

.image image {
  width: 240rpx;
  height: 6.4em;
  margin-right: 20rpx;
  border-radius: 6rpx;
}

.time{
  font-size: 22rpx;
  color: #cccccc;
  /* box-shadow: 0 2px 4px rgba(0, 0, 0, .12), 0 0 6px rgba(0, 0, 0, .04);  */
}

.footer{
  display: flex;
  justify-content: space-between;
